{
  "manifest_version": 2,
  "default_locale": "en",
  "icons": {
  },
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["scripts/contentscript.js"],
      "run_at": "document_end"
  }],
  "permissions": [
    "activeTab",
    "bookmarks",
    "contextMenus",
    "storage",
    "tabs"
  ]
}
